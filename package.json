{
  "publisher": "BE",
  "name": "ac-ui",
  "displayName": "ac_ui",
  "description": "test extension for ac ui",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "engines": {
    "vscode": "^1.74.0",
    "node": "^19.0.0",
    "npm": "^8.1.0",
    "yarn": "^3.0.1"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.BE2.ac-ui",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ac-ui",
          "title": "Autonomous Control",
          "icon": "resources\\TEL.svg"
        }
      ]
    },
    "views": {
      "ac-ui": [
        {
          "id": "ac-ui",
          "name": "Process Outline"
        }
      ],
      "explorer": [
        {
          "id": "ac-ui",
          "name": "Process Outline"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "ac-ui",
        "contents": "This is the process outline view. It shows the phases and transitions in the process.\n[Open file](command:workbench.action.files.openFile)\n",
        "when": "workbenchState == empty"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "tsc -b tsconfig.json",
    "build:force": "tsc -b --force tsconfig.json",
    "dev": "node ./out/main.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.1",
    "vscode-test": "^1.5.3",
    "@types/vscode": "^1.75.1"
  }
}
